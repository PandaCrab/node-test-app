{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "indent": ["error", 4],
        "linebreak-style": 0,
        "no-plusplus": 0,
        "max-len": ["error", 120],
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "input/newline-after-input": 0,
        "no-var": 1,
        "no-bitwise": 0
    }
}
